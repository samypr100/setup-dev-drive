    "rootDir": "./src",
    "outDir": "./lib",
    "forceConsistentCasingInFileNames": true,
    "newLine": "lf",
    "noImplicitAny": true,
    "sourceMap": true
  },
  "exclude": [
    "./lib",
    "./dist",
    "./node_modules"
  ]
}
